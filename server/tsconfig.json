{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
      "lib": [
      "dom",
      "es2017"
    ],
    "sourceMap": true,
    "allowJs": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "types": [
      "node"
    ]
  }
}